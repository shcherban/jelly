{
  "scripts": {
    "build": "npm run compile-coffee && npm run copy-html",
    "compile-coffee": "coffee --compile --transpile --inline-map --output build src/jelly.coffee",
    "copy-html": "cpx src/index.html build"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "coffeescript": "^2.7.0",
    "cpx": "^1.5.0"
  }
}
